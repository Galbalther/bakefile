module {
  variables {
  }
  targets {
    program normal {
      sources {
        file @top_srcdir/test.c
      }
    }
    program from_templ {
      defines = [COMPONENT="$(id)"]
      sources {
        file @top_srcdir/test.c
      }
    }
    program from_two_templs {
      defines = [COMPONENT="$(id)", SECOND_TEMPLATE]
      sources {
        file @top_srcdir/second_file.c
        file @top_srcdir/test.c
      }
    }
    program from_combined {
      defines = [COMPONENT="$(id)", SECOND_TEMPLATE]
      sources {
        file @top_srcdir/second_file.c
        file @top_srcdir/test.c
      }
    }
    program from_combined_dupl {
      defines = [COMPONENT="$(id)", SECOND_TEMPLATE]
      sources {
        file @top_srcdir/second_file.c
        file @top_srcdir/test.c
      }
    }
  }
}
